Actor TDBots_IDoExist : Inventory {}

ACTOR TDBots_NodeGetSpecial : CustomInventory
{
	+INVENTORY.AUTOACTIVATE
	States
	{
	Use:
		TNT1 A 0 ACS_NamedExecuteWithResult("TDBots_NodeGetSpecial", special)
		Stop
	}
}

ACTOR TDBots_PathNode_Map 2401
