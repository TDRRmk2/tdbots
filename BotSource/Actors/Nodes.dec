	//$Title Path node
	//$Category TDBots
	//$NotAngled
	//$Color 13
	
	+INVISIBLE
	+NOBLOCKMAP
	+NOINTERACTION
	RenderStyle None

	Gravity 0.0
	Radius 16
	Height 32
	States
	{
	Spawn:
		TBND B 2 NoDelay A_SpawnItemEx("TDBots_PathNode", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION|SXF_TRANSFERSPECIAL|SXF_TRANSFERAMBUSHFLAG)
